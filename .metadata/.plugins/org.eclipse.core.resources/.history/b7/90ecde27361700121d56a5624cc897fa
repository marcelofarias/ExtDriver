package com.sencha.extjs.driver;

public class ExtComponentFixture {
	
	private String id;
	private ExtDriver driver;
	
	public ExtComponentFixture(String id, ExtDriver driver) {
		this.id = id;
		this.driver = driver;
		String className = (String) driver.executeScript("return Ext.getCmp(argument[0]).$className", id);
	}
	
	public ExtDriver getDriver() {
		return this.driver;
	}

}
